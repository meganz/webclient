{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "safari": "14"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/transform-react-display-name",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ],
        ["@babel/plugin-transform-class-properties", { "loose" : true }]
      ]
    },
    "production": {
      "plugins": [
        "@babel/transform-react-display-name",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ],
        ["@babel/plugin-transform-class-properties", { "loose" : true }]
      ]
    }
  }
}
